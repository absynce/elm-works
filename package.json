  "bugs": {
    "url": "https://github.com/absynce/elm-works/issues"
  },
  "homepage": "https://github.com/absynce/elm-works#readme",
  "devDependencies": {
    "chokidar-cli": "^1.2.0",
    "elm": "^0.18.0"
  }
}
