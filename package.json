{
  "name": "elm-works",
  "version": "1.0.0",
  "description": "Presentation on using Elm at work written in Elm.",
  "main": "index.js",
  "scripts": {
    "build": "cd src && elm-make Main.elm --output=../index.html",
    "dev": "npm run watch -s && npm run start",
    "start": "elm reactor -p 8082",
