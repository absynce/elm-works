    "watch": "chokidar \"**/*.elm\" -c \"npm run build\" --initial"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/absynce/elm-works.git"
  },
  "keywords": [
    "elm",
    "presentation",
    "work"
  ],
  "author": "absynce@gmail.com",
